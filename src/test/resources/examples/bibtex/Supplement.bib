	url = {},
	keywords = {Base Composition;+ Ecology/Biodiversity;Base Sequence;+ Qualifying Exam Proposal;Research Support, Non-U.S. Gov't;Compositional Bias;Bacteria;Genome, Bacterial;Gene Frequency;Binning;Microbes;Bayes Theorem;Gene Transfer, Horizontal;Archaea;Genes, Bacterial;Lateral Transfer;Microbial Ecology;GC Rich Sequence},
