	volume = {228},
	number = {4},
	pages = {523-37},
	organization = {Laboratoire de Biométrie et Biologie Evolutive, UMR CNRS 5558, Université Claude Bernard LYON I, 43, boulevard du 11 novembre 1918, 69622 Villeurbanne Cedex, France. pavoine\@biomserv.univ-lyon1.fr},
