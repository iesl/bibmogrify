	url = {},
	keywords = {research support, non-u.s. gov't;research support, u.s. gov't, non-p.h.s.;Ribosomal RNA;Metagenomics;DNA, Ribosomal;Surveys;review;Cluster Analysis;Biodiversity;Bacteria;Microbes;RNA, Ribosomal, 16S;DNA, Bacterial},
