	title = {Winter forest soil respiration controlled by climate and microbial community composition.},
	journal = {Nature},
	uuid = {CA62E0DA-6067-4214-BA00-CB79E55CB06F},
	volume = {439},
	number = {7077},
	pages = {711-4},
