	keywords = {Sequence Analysis, DNA;Reproducibility of Results;+ Qualifying Exam Proposal;Base Sequence;Mitosis;Motifs;Models, Statistical;Monte Carlo Method;Models, Genetic;Sensitivity and Specificity;validation studies;+ Computational Methods;Computational Methods;Saccharomyces cerevisiae;+ Microarrays;+ Gene Expression;Gene Expression Regulation;Pattern Recognition, Automated;Amino Acid Motifs;Research Support, U.S. Gov't, P.H.S.;Molecular Sequence Data;Oligonucleotide Array Sequence Analysis},
	tags = {+ Microarrays;+ Gene Expression;Motifs;Computational Methods;+ Qualifying Exam Proposal;+ Computational Methods},
