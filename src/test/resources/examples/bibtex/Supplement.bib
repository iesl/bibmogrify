	url = {},
	keywords = {research support, non-u.s. gov't;Polymerase Chain Reaction;Animals;Reproducibility of Results;+ Ecology/Biodiversity;Ribosomal RNA;Evolution, Molecular;Taq Polymerase;Gene Library;Biodiversity;Errors;Plankton;Artifacts;RNA, Ribosomal, 16S;research support, u.s. gov't, non-p.h.s.},
