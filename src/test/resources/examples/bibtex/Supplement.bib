	sentelink = {file://localhost/Users/lorax/Documents/Sente/Local.sente6lib/Contents/Attachments/Sun/2009/17E20950-C92F-4E16-B9CD-39A1B5A4F771.pdf,DOI full text locator,Downloaded file},
