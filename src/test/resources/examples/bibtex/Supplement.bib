	url = {},
	keywords = {Codon;Base Sequence;DNA;+ Databases;Models, Genetic;RNA, Messenger;Species Specificity;Compositional Bias;Bacteria;Codon Usage;Genome, Bacterial;Temperature;Genome;Microbes;Databases;Genomic Signature;Purines},
