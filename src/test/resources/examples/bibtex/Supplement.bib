	journal = {J Virol},
	uuid = {4DBEE25B-AB6F-467B-9280-4F17AC852AA6},
	organization = {Blood Systems Research Institute, San Francisco, CA 94118, USA; Center for Infection and Immunity, Columbia University, New York 10032, NY, USA; University of Edinburgh, Edinburgh, Scotland, UK; National Institute of Health, Islamabad, Pakistan; Department of Laboratory Medicine, University of California, San Francisco, San, Francisco CA94118, USA.},
