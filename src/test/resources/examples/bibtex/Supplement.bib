@article{Mahadevan:AdvExpMedBiol:2010,
	author = {Mahadevan, Padmanabhan  and Seto, Donald},
	title = {Taxonomic Parsing of Bacteriophages Using Core Genes and In Silico Proteome-Based CGUG and Applications to Small Bacterial Genomes.},
