@article{Elshahed:ApplEnvironMicrobiol:2008,
	author = {Elshahed, Mostafa S.  and Youssef, Noha H.  and Spain, Anne M.  and Sheik, Cody  and Najar, Fares Z.  and Sukharnikov, Leonid O.  and Roe, Bruce A.  and Davis, James P.  and Schloss, Patrick D.  and Bailey, Vanessa L.  and Krumholz, Lee R.},
	title = {Novelty and uniqueness patterns of rare members of the soil biosphere.},
