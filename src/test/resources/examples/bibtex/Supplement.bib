@article{Fierer:ApplEnvironMicrobiol:2007,
	author = {Fierer, Noah  and Breitbart, Mya  and Nulton, James  and Salamon, Peter  and Lozupone, Catherine  and Jones, Ryan  and Robeson, Michael  and Edwards, Robert A.  and Felts, Ben  and Rayhawk, Steve  and Knight, Rob  and Rohwer, Forest  and Jackson, Robert B.},
