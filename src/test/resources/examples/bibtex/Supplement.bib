@article{VandePeer:JMolEvol:1993,
	author = {Van de Peer, Y.  and Neefs, J. M.  and De Rijk, P.  and De Wachter, R.},
	title = {Reconstructing evolution from eukaryotic small-ribosomal-subunit RNA sequences: calibration of the molecular clock.},
	journal = {J Mol Evol},
	uuid = {247E6C2E-0782-470C-ABBF-A762DAEDC0D2},
	volume = {37},
	number = {2},
	pages = {221-32},
	organization = {Department Biochemie, Universiteit Antwerpen (UIA), Belgium.},
