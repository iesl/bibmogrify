	keywords = {Base Sequence;Algorithms;Databases, Factual;Sequence Alignment;Mutation;+ Databases;Sensitivity and Specificity;+ Computational Methods;Computational Methods;Software;Sequence Homology, Nucleic Acid;Amino Acid Sequence;Research Support, U.S. Gov't, P.H.S.},
