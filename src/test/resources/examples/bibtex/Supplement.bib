@article{Valinsky:ApplEnvironMicrobiol:2002,
	author = {Valinsky, Lea  and Della Vedova, Gianluca  and Scupham, Alexandra J.  and Alvey, Sam  and Figueroa, Andres  and Yin, Bei  and Hartin, R. Jack  and Chrobak, Marek  and Crowley, David E.  and Jiang, Tao  and Borneman, James},
	title = {Analysis of bacterial community composition by oligonucleotide fingerprinting of rRNA genes.},
	journal = {Appl Environ Microbiol},
	uuid = {BDA66F87-445A-4070-9521-3D2197B661E8},
	volume = {68},
